# Configurações do Banco de Dados
DATABASE_URL=postgresql://usuario:senha@localhost:5432/karooma

# Autenticação Google OAuth
GOOGLE_CLIENT_ID=seu_google_client_id_aqui
GOOGLE_CLIENT_SECRET=seu_google_client_secret_aqui

# Sessão (gere uma chave aleatória forte)
SESSION_SECRET=sua_chave_secreta_super_forte_aqui

# Configuração do Servidor
NODE_ENV=production
PORT=3000

# Object Storage (se usar)
DEFAULT_OBJECT_STORAGE_BUCKET_ID=seu_bucket_id
PRIVATE_OBJECT_DIR=caminho_privado
PUBLIC_OBJECT_SEARCH_PATHS=caminhos_publicos

# Configuração PostgreSQL (Hostinger)
PGDATABASE=nome_do_banco
PGHOST=host_do_banco
PGPASSWORD=senha_do_banco
PGPORT=5432
PGUSER=usuario_do_banco